<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoDefaultLaunchSettingsFile>true</NoDefaultLaunchSettingsFile>
    <StaticWebAssetProjectMode>Default</StaticWebAssetProjectMode>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="9.0.11" />
    <PackageReference Include="Syncfusion.Blazor.AI" Version="*" />
	<PackageReference Include="Syncfusion.Blazor.Diagram" Version="*" />
	<PackageReference Include="Syncfusion.Blazor.Themes" Version="*" />
	<PackageReference Include="Azure.AI.OpenAI" Version="2.1.0" />
	<PackageReference Include="Microsoft.Extensions.AI" Version="10.1.1" />
	<PackageReference Include="Microsoft.Extensions.AI.OpenAI" Version="9.8.0-preview.1.25412.6" />
  </ItemGroup>

</Project>
